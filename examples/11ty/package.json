{
  "private": "true",
  "name": "example-with-11ty",
  "version": "1.0.0",
  "author": "Joe Bell <joe@joebell.co.uk>",
  "license": "Apache-2.0",
  "funding": "https://plaiceholder.co",
  "scripts": {
    "tw:build": "NODE_ENV=production postcss ./src/styles/index.css -o ./_site/styles/index.css",
    "tw:watch": "NODE_ENV=development postcss ./src/styles/index.css -o ./_site/styles/index.css -w",
    "11ty:watch": "yarn 11ty:build -- --serve --port=5000",
    "11ty:build": "eleventy --input=src",
    "build": "yarn tw:build && yarn 11ty:build",
    "dev": "concurrently \"yarn tw:watch\" \"yarn 11ty:watch\"",
    "clean": "rimraf _site",
    "vercel": "yarn --cwd ../../ prepare && yarn build"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@plaiceholder/tailwindcss": "^1.0.0",
    "plaiceholder": "^1.0.0",
    "autoprefixer": "10.2.5",
    "concurrently": "6.2.0",
    "clean-css": "4.2.3",
    "postcss": "8.2.9",
    "postcss-cli": "8.3.1",
    "serve": "11.3.2",
    "sharp": "0.28.2",
    "tailwindcss": "2.1.2"
  }
}
